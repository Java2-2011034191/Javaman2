		Car c1 = new Car("S600", "white", 80);
		Car c2 = new Car("E500", "blue", 20);
		// int n = Car.numbers; // 정적 변수
		int n = Car.getNumberOfCars();
		
		System.out.println("지금까지 생성된 자동차 수 = " + n);
	}
	
}
//numbers가 바로 클래스변수, 클래스 인스턴스를 만들지 않고서도 사용 가능
